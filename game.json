{
	"width": 1280,
	"height": 720,
	"fps": 30,
	"main": "./script/_bootstrap.js",
	"assets": {
		"player": {
			"type": "image",
			"width": 32,
			"height": 32,
			"path": "image/player.png"
		},
		"shot": {
			"type": "image",
			"width": 16,
			"height": 16,
			"path": "image/shot.png"
		},
		"se": {
			"type": "audio",
			"path": "audio/se",
			"systemId": "sound",
			"duration": 250
		},
		"_bootstrap": {
			"type": "script",
			"path": "script/_bootstrap.js",
			"global": true
		},
		"main": {
			"type": "script",
			"path": "script/main.js",
			"global": true
		},
		"parameterObject": {
			"type": "script",
			"path": "script/parameterObject.js",
			"global": true
		},
		"floor": {
			"type": "image",
			"path": "image/floor.png",
			"width": 2000,
			"height": 2000
		},
		"kaeru": {
			"type": "image",
			"path": "image/kaeru.png",
			"width": 1500,
			"height": 250
		},
		"title": {
			"type": "image",
			"path": "image/title.png",
			"width": 1034,
			"height": 720
		}
	},
	"moduleMainScripts": {},
	"environment": {
		"sandbox-runtime": "3",
		"niconico": {
			"supportedModes": [
				"multi_admission"
			],
			"preferredSessionParameters": {
				"totalTimeLimit": 7
			}
		}
	}
}